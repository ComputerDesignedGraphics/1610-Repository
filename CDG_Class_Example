public class CDG_ClassExample
{

}

namespace ClassWithVariables
{
  class Program
  {
    static void Main(string[] args)
    {
      int myNum = 57;
      Console.WriteLine(myNum);
     }
  }
}

namespace MyApplication
{
  class Program
  {
    static void Main(string[] args)
    {
      string name = "I Love Steaks";
      string name2 = "Baked Potato";
      Console.WriteLine(name);  
      Console.WriteLine(name2);  
     }
  }
}


